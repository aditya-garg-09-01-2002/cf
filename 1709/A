#include <bits/stdc++.h>

using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--)
    {
        int n , a ,b, c;
        cin>>n>>a>>b>>c;
        if(n==1)
        {
            if(a==0)
            cout<<"NO"<<endl;
            else if(a==2&&b==3)
            cout<<"YES"<<endl;
            else if(a==3&&c==2)
            cout<<"YES"<<endl;
            else
            cout<<"NO"<<endl;
        }
        else if(n==2)
        {
            if(b==0)
            cout<<"NO"<<endl;
            else if(b==3&&c==1)
            cout<<"YES"<<endl;
            else if(b==1&&a==3)
            cout<<"YES"<<endl;
            else
            cout<<"NO"<<endl;
        }
        else if(n==3)
        {
            if(c==0)
            cout<<"NO"<<endl;
            else if(c==2&&b==1)
            cout<<"YES"<<endl;
            else if(c==1&&a==2)
            cout<<"YES"<<endl;
            else
            cout<<"NO"<<endl;
        }
    }
}
